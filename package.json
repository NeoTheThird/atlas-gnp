{
  "name": "atlas-gnp",
  "version": "0.1.2",
  "description": "A map showing all Neuropaediatricians in Germany, Austria and Switzerland.",
  "main": "src/server.js",
  "scripts": {
    "start": "npx nodemon src/server.js",
    "lint": "npx prettier \"**\" --ignore-path=.gitignore --arrow-parens=avoid --trailing-comma=none -uw",
    "test": "npx mocha --recursive ./tests/unit-tests",
    "test-pipeline": "npx nyc --exclude=**/test_*.js --reporter=text --reporter=lcov ./node_modules/mocha/bin/mocha -- -R spec './tests/unit-tests/**'",
    "jsdoc": "npx jsdoc -c jsdoc-config.json"
  },
  "author": "Jan Sprinz <neo@neothethird.de>",
  "license": "GPL-3.0-or-later",
  "homepage": "http://atlas.gesellschaft-fuer-neuropaediatrie.org/",
  "repository": "https://github.com/neothethird/atlas",
  "dependencies": {
    "axios": "^0.21.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "node-geocoder": "^3.27.0",
    "node-storage": "0.0.9"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.1.0",
    "jsdoc": "^3.6.4",
    "mocha": "^7.1.2",
    "nodemon": "^2.0.3",
    "nyc": "^15.0.1",
    "prettier": "^2.3.1",
    "proxyquire": "^2.1.3",
    "sinon": "^8.1.1",
    "sinon-chai": "^3.5.0"
  }
}
